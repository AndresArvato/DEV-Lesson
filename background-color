<?php
  	$wuerfel
  
  	$wuerfel = 10
  
  	if ($wuerfel = 10) {
	$wuerfel = 'A';
  	} elseif ($wuerfel = 11) {
	$wuerfel = 'B';
  	} elseif ($wuerfel = 12) {
	$wuerfel = 'C';
  	} elseif ($wuerfel = 13) {
	$wuerfel = 'D';
  	} elseif ($wuerfel = 14) {
	$wuerfel = 'E';
  	} else ($wuerfel = 15) {
	$wuerfel = 'F';
  	}

 	echo ($wuerfel);
?>
